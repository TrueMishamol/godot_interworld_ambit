config_version=5

[application]

config/name="Interworld Ambit"
config/description="Slippy Ambit - don't fly out!"
config/version="0.3.0"
run/main_scene="res://levels/menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.211765, 0.215686, 0.254902, 1)
boot_splash/show_image=false
boot_splash/image="res://ui/interworld_ambit.png"
boot_splash/fullsize=false
config/icon="res://ui/interworld_ambit.png"

[autoload]

ApplicationControls="*res://scripts/autoload/application_controls.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
